@model Estabelecimento
@{
    ViewData["Title"] = "Cadastro de restaurante";
}


<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Wendy+One&display=swap" rel="stylesheet">

<link href="~/css/master.css" rel="stylesheet" />
<link href="~/css/form-card.css" rel="stylesheet" />
<link href="~/css/index-about.css" rel="stylesheet" />

<div class="page-content">
    <partial name="_LeftContent" />
    <div class="right-content" style="display: flex; flex-direction: column; justify-content: center; margin-left: 8%;">
        <Feedback Texto="@TempData["Erro"]" is-error="true"></Feedback>
        <h1 style="display: flex; justify-content: center;" class="my-5">Cadastre um estabelecimento</h1>
        <div class="card cardStyle mx-auto" style="width: 70%">
            <form asp-action="CadastroEstabelecimento">
                <partial name="_FormCreateUpdateEstabelecimento" />

                <div class="form-row formRowStyleButton">
                    <button onclick="goBack()" class="btn btn-danger px-5">Cancelar</button>
                    <input type="submit" value="Próxima etapa" class="btn btn-info ml-3 px-5" />
                </div>
            </form>
        </div>
    </div>
</div>
